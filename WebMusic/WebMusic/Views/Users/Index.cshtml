                @Html.DisplayFor(modelItem => item.UsStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Photo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pass2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Question)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UsId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UsId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UsId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
